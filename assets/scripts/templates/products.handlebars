<div id="template-title">PAST PROJECTS</div>
<div id="create-product">
  <button id="new-prod"class="btn btn-default test-button" data-toggle="modal" data-dismiss="modal" aria-hidden="true">Create New Product</button>
</div>
<hr>
{{#each products as |product|}}
<ul class=handlebars-body>
<h4 id="product-description">Product Description: {{product.description}}</h4>
<h5>Price: ${{product.price}}</h5>
<h5>Before-Photo Link: {{product.before_photo_url}}
<h5>After-Photo Link: {{product.after_photo_url}}
<h6>Product Number: {{product.id}}
<hr>
  <button id="edit-product" class="btn btn-default test-button" data-toggle="modal" data-dismiss="modal" aria-hidden="true" href="#myModal5">Edit Product</button>
  <button id="delete-product" class="btn btn-danger test-button" data-toggle="modal" data-dismiss="modal" aria-hidden="true" href="#myModal5">Delete Product</button>
</ul>
<hr>
{{/each}}
